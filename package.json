{
  "name": "agentweb",
  "version": "0.1.0",
  "description": "Web interface for the Claude Agent SDK",
  "type": "module",
  "main": "dist-server/server.js",
  "bin": {
    "agentweb": "./bin/agentweb.js"
  },
  "scripts": {
    "dev": "npx vite",
    "build": "npx tsc -b && npx vite build",
    "build:server": "npx tsc -p tsconfig.server.json",
    "build:all": "npm run build && npm run build:server",
    "prestart": "npm run build:server",
    "start": "node dist-server/server.js",
    "prestart:stdio": "npm run build:server",
    "start:stdio": "node dist-server/stdio.js",
    "preview": "npx vite preview",
    "test": "npx vitest run",
    "test:watch": "npx vitest",
    "test:coverage": "npx vitest run --coverage",
    "lint": "npx eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "npx eslint . --ext .ts,.tsx --fix",
    "typecheck": "npx tsc --noEmit && npx tsc -p tsconfig.server.json --noEmit"
  },
  "dependencies": {
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^24.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "claude",
    "agent",
    "sdk",
    "web",
    "ui"
  ],
  "license": "MIT"
}
